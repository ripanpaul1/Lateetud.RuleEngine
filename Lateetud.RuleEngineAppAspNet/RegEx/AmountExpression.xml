<?xml version="1.0" encoding="utf-8" ?>
<RegEx>
  <Exp ID="1" Title="Amount">
    <![CDATA[^(\$)?((\d{1,5})|(\d{1,3})(\,\d{3})*)(\.\d{1,2})?$]]>
  </Exp>
  <Exp ID="2" Title="Amount">
    <![CDATA[^\$?\d{1,2}\,\d{3}?\,\d{3}?(\.(\d{2}))$|^\$?\d{1,3}?\,\d{3}?(\.(\d{2}))$|^\$?\d{1,3}?(\.(\d{2}))$]]>
  </Exp>
  <Exp ID="3" Title="Amount">
    <![CDATA[$(\d+)]]>
  </Exp>
  <Exp ID="4" Title="Amount">
    <![CDATA[(\\$\\w+)]]>
  </Exp>
  <Exp ID="5" Title="Amount">
    <![CDATA[(\\$[a-zA-Z]*)]]>
  </Exp>
 <!-- <Exp ID="6" Title="Amount">
    <![CDATA[$]]>
  </Exp>-->
</RegEx>